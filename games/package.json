{
	"name": "game-router",
	"version": "0.1.0",
	"private": true,
	"homepage": "http://www.mikeshort.me",
	"dependencies": {
		"braces": "^2.3.2",
		"react": "^16.8.1",
		"react-bootstrap": "^1.0.0-beta.5",
		"react-dom": "^16.8.1",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-scripts": "2.1.3"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"proxy": "http://localhost:3001",
	"jest": {
		"setupFiles": [
			"./games/src/setupTests.js"
		],
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		],
		"moduleNameMapper": {
			"^.+\\.(css|scss)$": "identity-obj-proxy"
		}
	}
}
