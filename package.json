{
	"name": "profile-page",
	"version": "0.0.3",
	"description": "Personal website for job hunting",
	"main": "server.js",
	"scripts": {
		"start": "node server.js && cd games && npm start",
		"heroku-postbuild": "echo \"start postbuild\" && cd games && npm install && npm run build && echo \"end postbuild\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"prettier-watch": "onchange '**/*.js' '**/*.jsx' '**/*.json' -- prettier --write {{changed}}",
		"build-css": "node-sass --include-path scss public/scss/home.scss public/css/home.css -w"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mikeshort10/mike-short"
	},
	"author": "Mike Short",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mikeshort10/mike-short/issues"
	},
	"homepage": "http://www.mikeshort.me",
	"dependencies": {
		"@babel/core": "^7.4.3",
		"@babel/preset-env": "^7.4.3",
		"babel-plugin-transform-export-extensions": "^6.22.0",
		"body-parser": "^1.18.3",
		"bootstrap": "^4.3.1",
		"concurrently": "^4.1.0",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"express-subdomain": "^1.0.5",
		"helmet": "^3.15.0",
		"identity-obj-proxy": "^3.0.0",
		"node-sass": "^4.12.0",
		"nodemailer": "^5.1.1",
		"onchange": "^5.2.0",
		"path": "^0.12.7",
		"pug": "^2.0.3",
		"react": "^16.8.1",
		"react-bootstrap": "^1.0.0-beta.5",
		"react-dom": "^16.8.1",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1"
	},
	"prettier": {
		"tabWidth": 4,
		"useTabs": true,
		"trailingComma": "all",
		"bracketSpacing": true
	},
	"devDependencies": {
		"browser-sync": "^2.26.3",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.12.1",
		"enzyme-to-json": "^3.3.5",
		"jest": "^23.6.0",
		"jest-cli": "^24.9.0",
		"jest-environment-enzyme": "^7.0.2",
		"jest-enzyme": "^7.0.2",
		"react-test-renderer": "^16.8.6"
	}
}
